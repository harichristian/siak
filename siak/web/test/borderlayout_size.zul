</borderlayout>
</center>
<south size="40%" flex="true" splittable="true"
       collapsible="true">
    <div align="center">
        <chart id="barchart3d" title="Top 10 Sales (Rp)"
               width="1000px" height="350px" type="bar" threeD="true"
               fgAlpha="128" model="@{catmodel}"/>
    </div>
</south>
</borderlayout>
</window>
</zk>